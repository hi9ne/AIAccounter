        "query": "=WITH month_mapping AS (\n  SELECT * FROM (VALUES\n    ('—è–Ω–≤–∞—Ä—å', 1), ('—Ñ–µ–≤—Ä–∞–ª—å', 2), ('–º–∞—Ä—Ç', 3), ('–∞–ø—Ä–µ–ª—å', 4),\n    ('–º–∞–π', 5), ('–∏—é–Ω—å', 6), ('–∏—é–ª—å', 7), ('–∞–≤–≥—É—Å—Ç', 8),\n    ('—Å–µ–Ω—Ç—è–±—Ä—å', 9), ('–æ–∫—Ç—è–±—Ä—å', 10), ('–Ω–æ—è–±—Ä—å', 11), ('–¥–µ–∫–∞–±—Ä—å', 12)\n  ) AS m(month_name, month_num)\n)\nSELECT\n  TO_CHAR(t.date, 'DD.MM.YYYY') as date,\n  t.category,\n  t.amount,\n  t.description,\n  COUNT(*) OVER() as total_rows,\n  SUM(t.amount) OVER() as total_amount\nFROM {{ $fromAI(\"type\", \"–¢–∏–ø –æ–ø–µ—Ä–∞—Ü–∏–∏: '—Ä–∞—Å—Ö–æ–¥' –∏–ª–∏ '–¥–æ—Ö–æ–¥'\") === '—Ä–∞—Å—Ö–æ–¥' ? 'expenses' : 'income' }} t\nINNER JOIN month_mapping mm ON EXTRACT(MONTH FROM t.date) = mm.month_num\nWHERE t.user_id = 1109421300\n  AND EXTRACT(YEAR FROM t.date) = EXTRACT(YEAR FROM CURRENT_DATE)\n  AND LOWER(mm.month_name) = LOWER('{{ $fromAI(\"month\", \"–ù–∞–∑–≤–∞–Ω–∏–µ –º–µ—Å—è—Ü–∞ –Ω–∞ —Ä—É—Å—Å–∫–æ–º —è–∑—ã–∫–µ (–Ω–∞–ø—Ä–∏–º–µ—Ä: –æ–∫—Ç—è–±—Ä—å, –Ω–æ—è–±—Ä—å)\") }}')\n  {% if $fromAI(\"category\", \"–ö–∞—Ç–µ–≥–æ—Ä–∏—è –¥–ª—è —Ñ–∏–ª—å—Ç—Ä–∞—Ü–∏–∏ (–æ–ø—Ü–∏–æ–Ω–∞–ª—å–Ω–æ)\", \"\") %}\n  AND t.category = '{{ $fromAI(\"category\", \"–ö–∞—Ç–µ–≥–æ—Ä–∏—è\", \"\") }}'\n  {% endif %}\nORDER BY t.date DESC;",
